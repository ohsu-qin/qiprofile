.qi-subtable-title TNM

.qi-panel.qi-nested(ng-controller='TNMCtrl')
  table.qi-table
    tr
      th Stage
      td {{ tnm | tnmStage | romanize }}

  .qi-bottom-buffer(ng-show='!!tnm.grade')
    include grade

  table.qi-table.qi-spaced-table
    tr
      th Size
      td {{ tnm.size | tnmSize }}
    tr
      th Lymph Status
      td {{ tnm.lymphStatus | notSpecified }}
    tr
      th Metastasis
      td {{ tnm.metastasis | presentAbsent }}
