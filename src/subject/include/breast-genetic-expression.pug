.qi-table-title Genetic Expression

.qi-panel.qi-nested
  table.table.qi-table.qi-vertical-space(
    *ngIf="has(tumor.geneticExpression.her2NeuIhc) || has(tumor.geneticExpression.her2NeuFish) || has(tumor.geneticExpression.ki67)"
  )
    tr(*ngIf="has(tumor.geneticExpression.her2NeuIhc)")
      th HER2 NEU IHC
      td {{ tumor.geneticExpression.her2NeuIhc }}
    tr(*ngIf="has(tumor.geneticExpression.her2NeuFish)")
      th HER2 NEU FISH
      td {{ tumor.geneticExpression.her2NeuFish | boolean }}
    tr(*ngIf="has(tumor.geneticExpression.ki67)")
      th Ki67
      td {{ tumor.geneticExpression.ki67 }}

  .qi-vertical-space(*ngIf="has(tumor.geneticExpression.normalizedAssay)")
    include breast-gene-assay
