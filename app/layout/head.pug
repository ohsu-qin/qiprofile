head
  meta(charset='utf-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge,chrome=1')
  meta(name='viewport' content='width=device-width, initial-scale=1')
  meta(name='keywords' content='QIN MR imaging')
  meta(name='description' content='Imaging Profile - display imaging and clinical data.')

  script(
    data-main='/javascripts/main',
    src='//cdnjs.cloudflare.com/ajax/libs/require.js/2.1.22/require.js'
  )
  //
    If the CloudFlare CDN requirejs host is down, then fall back to the
    server installed copy.
  script
    | if (typeof define == 'undefined' || !define.amd) {
    |   document.write(
    |     unescape("%3Cscript data-main='/javascripts/main', src='/javascript/lib/require.js'%3E")
    |   );
    | }

  link(href='/static/stylesheets/app.css' rel='stylesheet')
  link(href='/static/media/favicon.ico' rel='shortcut icon' type='image/x-icon')

  title
    block title
      | quip - Quantitative Imaging Profile
