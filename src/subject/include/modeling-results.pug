// The PK modeling results.
.qi-panel(*ngIf="has(subject.modelings)")
  h4.panel-title Pharmacokinetic Modeling
  div(*ngIf="subject.isMultiSession()")
    include modelingParameters
    button.btn.btn-sm.qi-btn(
      *ngIf="subject.isMultiSession()",
      (click)="toggleModelingFormat()"
    )
      .fa.fa-bar-chart(*ngIf="modelingFormat == 'chart'")
      .fa.fa-list-ul(*ngIf="modelingFormat == 'table'")
    button.btn.btn-med.qi-btn.qi-minihelp-btn(
        (click)="openModelingHelp()"
      )
        .glyphicon.glyphicon-question-sign
    accordion([showArrows]=true)
      accordion-group(*ngFor="param in modelingParameters()")
      [heading]="'Demographics'")
        include demographics
  div(*ngIf="!subject.isMultiSession()")
.panel.panel-default(*ngIf="has(subject.modelings)")
  .panel-heading
    h4.panel-title Pharmacokinetic Modeling
      button.btn.btn-sm.qi-btn(
        *ngIf="subject.isMultiSession()",
        (click)="toggleModelingFormat()"
      )
        .fa.fa-bar-chart(*ngIf="modelingFormat == 'chart'")
        .fa.fa-list-ul(*ngIf="modelingFormat == 'table'")
      button.btn.btn-med.qi-btn.qi-minihelp-btn(
          (click)="openModelingHelp()"
        )
          .glyphicon.glyphicon-question-sign


  //- .panel-collapse
  //-   .panel-body
  //-     include modeling-selection
  //-     include modeling-parameters


accordion([showArrows]=true, [expandAll]=true)
  accordion-group([heading]="'Demographics'")
    include demographics

  // Iterate over the encounters to include the encounter partial.
  accordion-group(
    *ngFor="let encounter of subject.clinicalEncounters",
    [heading]="encounter.title", [isOpened]=true
  )
    include encounter
accordion(close-others='false')
  // The Ktrans table.
  accordion-group([heading]="encounter.title", [isOpened]=true)
    include k-trans

  // The v_e table.
  div(ng-controller='AccordionGroupCtrl')
    accordion-group(is-open="isOpen")
      include v-e

  // The tau_i table.
  div(ng-controller='AccordionGroupCtrl')
    accordion-group(is-open="isOpen")
      include tau-i
