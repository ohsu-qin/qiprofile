include accordion-heading-mixin

+accordion-heading('Demographics')

table#demographics.qi-table
  tr(ng-show="subject.birthDate != null")
    th Age
    td {{ subject.birthDate | age }}
  tr(ng-show="subject.gender != null")
    th.col Gender
    td.col {{ subject.gender }}
  tr(ng-show="subject.weight != null")
    th.col Weight
    td.col {{ subject.weight }}
  tr(ng-show="subject.races != null && subject.races.length > 0")
    th.col Race
    td.col {{ subject.races | races }}
  tr(ng-show="subject.ethnicity != null")
    th.col Ethnicity
    td.col {{ subject.ethnicity | ethnicity }}
