div(ng-controller='NecrosisPercentCtrl' ng-switch="necrosis_percent._cls")
  div(ng-switch-when='NecrosisPercentValue')
    table.table.qi-table
      tr
        th Necrosis Percent
        td {{ necrosis_percent.value }}

  div(ng-switch-when='NecrosisPercentRange')
    table.table.qi-table
      tr
        th Necrosis Percent
        td {{ necrosis_percent | percentRange }}
