{
  "name": "qiprofile",
  "version": "2.1.1",
  "description": "Quantitative Imaging Profile (qipr) web application",
  "repository": {
    "type": "git",
    "url": "git@github.com:ohsu-qin/qiprofile.git"
  },
  "keywords": [
    "qipr",
    "QIN",
    "DCE",
    "MR",
    "medical",
    "imaging"
  ],
  "author": "Fred Loney <loneyf@ohsu.edu>",
  "license": "MIT",
  "bin": "./bin/qiprofile",
  "scripts": {
    "postinstall": "./node_modules/grunt-cli/bin/grunt postinstall",
    "start": "node server.js",
    "test": "echo \"Error: No test specified\" && exit 1"
  },
  "homepage": "http://qiprofile.readthedocs.org/en/latest/",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "engines": {
    "node": ">=5.1"
  },
  "//": [
    "Note:",
    "jpsm consists of the client libraries.",
    "The remaining npm dependencies consist of the server and test",
    "libraries."
  ],
  "jspm": {
    "main": "main",
    "directories": {
      "packages": "_public/javascripts/lib"
    },
    "configFile": "app/javascripts/config.js",
    "dependencies": {
      "@angular2-material/button": "npm:@angular2-material/button@^2.0.0-alpha.3",
      "@angular2-material/card": "npm:@angular2-material/card@^2.0.0-alpha.3",
      "@angular2-material/checkbox": "npm:@angular2-material/checkbox@^2.0.0-alpha.3",
      "@angular2-material/core": "npm:@angular2-material/core@^2.0.0-alpha.3",
      "@angular2-material/input": "npm:@angular2-material/input@^2.0.0-alpha.3",
      "@angular2-material/list": "npm:@angular2-material/list@^2.0.0-alpha.3",
      "@angular2-material/progress-bar": "npm:@angular2-material/progress-bar@^2.0.0-alpha.3",
      "@angular2-material/progress-circle": "npm:@angular2-material/progress-circle@^2.0.0-alpha.3",
      "@angular2-material/radio": "npm:@angular2-material/radio@^2.0.0-alpha.3",
      "@angular2-material/sidenav": "npm:@angular2-material/sidenav@^2.0.0-alpha.3",
      "@angular2-material/toolbar": "npm:@angular2-material/toolbar@^2.0.0-alpha.3",
      "angular2": "npm:angular2@^2.0.0-beta.17",
      "es6-promise": "npm:es6-promise@^3.0.2",
      "es6-shim": "github:es-shims/es6-shim@^0.35.1",
      "json": "github:systemjs/plugin-json@^0.1.0",
      "lodash": "npm:lodash@^4.12.0",
      "ng2-bootstrap": "npm:ng2-bootstrap@^1.0.16",
      "ng2-translate": "npm:ng2-translate@^1.10.0",
      "reflect-metadata": "npm:reflect-metadata@^0.1.3",
      "typescript": "npm:typescript@^1.8.10",
      "zone.js": "npm:zone.js@^0.6.10"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha"
    },
    "overrides": {
      "npm:@angular/common@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/compiler@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/core@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser-dynamic@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:angular2@2.0.0-beta.17": {
        "main": false,
        "dependencies": {
          "reflect-metadata": "npm:reflect-metadata@0.1.2",
          "rxjs": "npm:rxjs@^5.0.0-beta.2",
          "zone.js": "npm:zone.js@^0.6.6"
        },
        "peerDependencies": {
          "reflect-metadata": "npm:reflect-metadata@0.1.2",
          "rxjs": "npm:rxjs@^5.0.0-beta.2",
          "zone.js": "npm:zone.js@^0.6.6"
        },
        "jspmNodeConversion": false,
        "map": {
          "crypto": "@empty"
        },
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:moment@2.13.0": {
        "jspmNodeConversion": false
      },
      "npm:reflect-metadata@0.1.2": {
        "map": {
          "crypto": "@empty"
        }
      },
      "npm:reflect-metadata@0.1.3": {
        "jspmNodeConversion": false,
        "map": {
          "crypto": {
            "node": "@node/crypto",
            "default": "@empty"
          }
        }
      }
    }
  },
  "dependencies": {
    "authenticate": "^0.1.5",
    "errorhandler": "^1.4.3",
    "express-bunyan-logger": "^1.2.0",
    "forever-monitor": "^1.7.0",
    "grunt-express-server": "^0.5.3",
    "method-override": "^2.3.5",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.2",
    "superagent": "^1.8.3"
  },
  "devDependencies": {
    "angular2": "^2.0.0-beta.17",
    "autoprefixer-stylus": "^0.9.3",
    "bootstrap": "^4.0.0-alpha.2",
    "csso-stylus": "0.0.3",
    "font-awesome": "^4.6.1",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-concurrent": "^2.3.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-coffee": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jade": "^1.0.0",
    "grunt-contrib-stylus": "^1.2.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-env": "^0.4.4",
    "grunt-exec": "^0.4.7",
    "grunt-markdown": "^0.7.0",
    "grunt-preprocess": "^5.1.0",
    "grunt-ts": "^5.5.0-beta.2",
    "jspm": "^0.16.34",
    "jstransformer-typescript": "^1.0.0",
    "load-grunt-tasks": "^3.5.0",
    "protractor": "^3.3.0",
    "rxjs": "^5.0.0-beta.7"
  },
  "main": "server.js"
}
